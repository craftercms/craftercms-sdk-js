{
    "extends": [
      "tslint-react"
    ],
    "rulesDirectory": [
      
    ],
    "rules": {
      "file-header": false,
      "no-console": [false, "log"], // TODO
      "no-duplicate-imports": true,
      "no-duplicate-variable": true,
      "no-jasmine-focus": true,
      "no-var-keyword": true,
      "require-internal-with-underscore": false,
      "rollup-config": true,
      "semicolon": [true],
      "variable-name": [true, "ban-keywords"],
      "no-inner-declarations": [true, "function"]
    },
    "jsRules": {
      "file-header": false,
      "no-console": [true, "log"],
      "no-duplicate-imports": true,
      "no-duplicate-variable": true,
      "no-jasmine-focus": true,
      "require-internal-with-underscore": false,
      "semicolon": [true],
      "variable-name": [true, "ban-keywords"],
      "no-inner-declarations": [true, "function"]
    }
  }